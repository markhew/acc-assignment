client: error.o wrapunix.o wrapsock.o wraplib.o wrapstdio.o mnc_client.o readline.o writen.o tcp_listen.o wrappthread.o mncclithread.o
	cc -o client wrappthread.o mncclithread.o error.o wrapunix.o wrapsock.o wraplib.o wrapstdio.o mnc_client.o readline.o writen.o tcp_listen.o -lpthread

error.o: error.c acc.h
	cc -c -w error.c

tcp_listen.o: tcp_listen.c acc.h
	cc -c -w tcp_listen.c

